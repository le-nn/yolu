
{
    "metadata": [
        {
            "src": [
                {
                    "src": "./src",
                    "files": [
                        "**/*.csproj"
                    ]
                }
            ],
            "dest": "api"
        }
    ],
    "build": {
        "content": [
            {
                "files": [
                    "**/*.{md,yml}"
                ],
                "exclude": [
                    "_site/**"
                ]
            }
        ],
        "resource": [
            {
                "files": [
                    "images/**"
                ]
            }
        ],
        "output": "./artifacts/_site",
        "template": [
            "default",
            "templates"
        ],
        "globalMetadata": {
            "_appName": "Yolu",
            "_appTitle": "Yolu",
            "_appFaviconPath": "logo.png",
            "_appLogoPath": "logo.png",
            "_appLogoUrl": "https://le-nn.github.io/yolu/",
            "_appFooter": "Copyright Â© 2024 le-nn",
            "_enableSearch": true,
            "pdf": false
        }
    }
}
